{
  "name": "monitor-a-list-from-redis",
  "version": "1.0.0",
  "description": "A simple tool to watch some realtime data from a redis source",
  "main": "count.js",
  "scripts": {
    "tslint": "tslint 'static/index.ts' 'count.ts'",
    "init": "npm i -g typescript@beta tslint npm-check eslint uglify-js clean-css rev-static",
    "bundle-index-js": "uglifyjs static/index.js -o static/index.bundle.js",
    "bundle-common-js": "uglifyjs ./node_modules/vue/dist/vue.min.js ./node_modules/moment/min/moment.min.js ./node_modules/chart.js/dist/Chart.min.js ./node_modules/socket.io-client/socket.io.js -o static/common.bundle.js",
    "bundle-index-css": "cleancss static/index.css -o static/index.bundle.css",
    "bundle-common-css": "cleancss ./node_modules/bootstrap/dist/css/bootstrap.min.css -o static/common.bundle.css",
    "rev": "rev-static static/common.bundle.css static/index.bundle.css static/common.bundle.js static/index.bundle.js static/index.ejs.html -o static/index.html",
    "build": "npm run bundle-common-js && npm run bundle-common-css && npm run bundle-index-js && npm run bundle-index-css && npm run rev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/monitor-a-list-from-redis.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/plantain-00/monitor-a-list-from-redis/issues"
  },
  "homepage": "https://github.com/plantain-00/monitor-a-list-from-redis#readme",
  "dependencies": {
    "express": "^4.13.4",
    "ioredis": "^2.0.0",
    "socket.io": "^1.4.6"
  },
  "devDependencies": {
    "@types/express": "^4.0.29",
    "@types/ioredis": "0.0.15",
    "@types/minimist": "^1.1.28",
    "@types/moment": "^2.11.28",
    "@types/socket.io": "^1.4.26",
    "@types/socket.io-client": "^1.4.26",
    "@types/vue": "^1.0.26",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.2.0",
    "moment": "^2.14.1",
    "socket.io-client": "^1.4.8",
    "typescript": "^2.0.0",
    "vue": "^1.0.26"
  }
}
